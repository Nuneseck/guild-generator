                    <property name="text" translatable="yes">Gerar Guilda</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="pady">5</property>
                      <property name="row">1</property>
                    </layout>
                  </object>
                </child>
                <child>
